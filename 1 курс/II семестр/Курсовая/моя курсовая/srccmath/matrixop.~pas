unit matrixop;

interface
Type
    TVector=array[0..1] of double;
    TMatrix=array[0..1] of TVector;

implementation

procedure VecVecT(u,v:TVector;var r:TMatrix);
begin
r[0][0]:=u[0]*v[0];
r[0][1]:=u[0]*v[1];
r[1][0]:=u[1]*v[0];
r[1][1]:=u[1]*v[1];
end;

