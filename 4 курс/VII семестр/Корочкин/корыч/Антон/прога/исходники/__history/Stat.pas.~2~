unit Stat;

interface

uses
  Windows, Messages, SysUtils, Variants, Classes, Graphics, Controls, Forms,
  Dialogs, Grids;

type
  TForm3 = class(TForm)
    StringGrid1: TStringGrid;
    procedure FormCreate(Sender: TObject);
  private
    { Private declarations }
  public
    { Public declarations }
  end;

var
  Form3: TForm3;

implementation

{$R *.dfm}

procedure TForm3.FormCreate(Sender: TObject);
begin
   StringGrid1.Cells[0,0] := 'Клиенты';
   StringGrid1.Cells[1,0] := 'Пришел в';
   StringGrid1.Cells[2,0] := 'Время ожидания';
   StringGrid1.Cells[3,0] := 'Время обслуживания';
   StringGrid1.Cells[4,0] := 'Время за столом';
   StringGrid1.Cells[5,0] := 'Общеее время в кафе';
   StringGrid1.Cells[6,0] := 'Ушел в';
   StringGrid1.TopRow :=0;
end;

end.
